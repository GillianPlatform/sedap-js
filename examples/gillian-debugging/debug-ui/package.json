{
  "name": "gillian-debugger-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --build && vite build",
    "watch:tsc": "tsc --build --watch --preserveWatchOutput",
    "watch:vite": "vite build --watch",
    "watch": "concurrently -n tsc,vite -c cyan,green \"npm run watch:tsc\" \"npm run watch:vite\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@gillianplatform/sedap-react": "0.0.1",
    "@gillianplatform/sedap-vscode-ui": "0.0.1",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "styled-components": "^6.1.14"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@gillianplatform/sedap-types": "0.0.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
